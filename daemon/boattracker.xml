<service>
	<id>boattracker.exe</id>
	<name>BoatTracker</name>
	<description>BoatTracker RFID Reader</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Users\winPC\AppData\Roaming\npm\node_modules\qckwinsvc\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>c:\repos\BoatTrackerRFID\app.js</argument>
	<argument>--log</argument>
	<argument>BoatTracker wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>C:\Repos\BoatTrackerRFID</workingdirectory>
</service>